{
  "manifest_version": 2,
  "name": "Love Poems AI for Edgar",
  "short_name": "love-edgar",
  "version": "1.0.0",
  "author": "Elizabeth <ekeleshian@gmail.com>",
  "description": "A custom language model trained over hand picked poems that remind Elizabeth of Edgar.",
  "browser_action": {
    "default_title": "Love Poem Generator",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    "dist/extension.js", "our_logo_32x32.png", "HomemadeApple-Regular.ttf", "personal_poem.jpg", "IMG_8427_2_300x235.png"
  ],
  "permissions": [
    "https://*/*", "activeTab", "tabs", "webNavigation", "storage"
  ]
}
